#Ajouter les ingrédients
curl -d "id=12&name=reblochon" -i localhost:8080/api/v1/ingredients --noproxy localhost
curl -d "id=13&name=poulycrock" -i localhost:8080/api/v1/ingredients --noproxy localhost

#Afficher la liste d'ingrédients
curl -i localhost:8080/api/v1/ingredients/ --noproxy localhost

#Afficher un ingrédient particulié
curl -i localhost:8080/api/v1/ingredients/11 --noproxy localhost

#Détruire un ingrédient
curl -X DELETE -vi localhost:8080/api/v1/ingredients/11 --noproxy localhost